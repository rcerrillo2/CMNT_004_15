<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="product_template_form_view_add_versions_page" model="ir.ui.view">
            <field name="name">product.template.common.form.add_versions_page</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page string="Versions">
                        <group colspan="4">
                            <field name="version_ids" nolabel="1" colspan="4">
                                <tree editable="top">
                                    <field name="version"/>
                                    <field name="name"/>
                                </tree>
                            </field>
                        </group>
                    </page>
                </notebook>
            </field>
        </record>

    </data>
</odoo>
